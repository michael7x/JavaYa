package capitulo13;

import java.util.Scanner;

public class CadenaDeCaracteres3 {
	public static void main(String[] arg){
		Scanner teclado = new Scanner(System.in);
		String apellido1, apellido2;
		
		System.out.print("Ingrese su primer apellido: ");
		apellido1 = teclado.next();
		
		System.out.print("Ingrese su segundo apellid: ");
		apellido2 = teclado.next();
		
		if(apellido1.equals(apellido2)){
			System.out.println("Los apellidos son iguales");
		} else {
			System.out.println("Los apellidos son distintos");
		}
	}

}
